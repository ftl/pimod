    <xs:attributeGroup ref="atts.data"/>
    <xs:attributeGroup ref="att.nodeid"/>
    <xs:attributeGroup ref="att.translate"/>
    <xs:attributeGroup ref="att.valid"/>
  </xs:attributeGroup>
  <xs:element name="data">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cm.data">
