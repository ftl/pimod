    <xs:attributeGroup ref="att.nodeid"/>
    <xs:attributeGroup ref="att.translate"/>
    <xs:attributeGroup ref="att.valid"/>
  </xs:attributeGroup>
  <xs:element name="actiondescription">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cm.actiondescription">
          <xs:attributeGroup ref="attlist.actiondescription"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.actiondescription">
    <xs:attributeGroup ref="attl.actiondescription"/>
  </xs:attributeGroup>
  <!-- **** Element <actionsequenceend> **** -->
  <xs:complexType name="cm.actionsequenceend">
    <xs:sequence>
      <xs:element minOccurs="0" ref="result"/>
      <xs:element minOccurs="0" ref="troubleshooting"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="note"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="attl.actionsequenceend">
    <xs:attributeGroup ref="att.nodeid"/>
    <xs:attributeGroup ref="att.translate"/>
    <xs:attributeGroup ref="att.valid"/>
  </xs:attributeGroup>
