    <xs:sequence>
      <xs:element minOccurs="0" ref="condition"/>
      <xs:element ref="step"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="attl.action">
    <xs:attributeGroup ref="att.nodeid"/>
    <xs:attributeGroup ref="att.translate"/>
    <xs:attributeGroup ref="att.valid"/>
  </xs:attributeGroup>
  <xs:element name="action">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cm.action">
          <xs:attributeGroup ref="attlist.action"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.action">
    <xs:attributeGroup ref="attl.action"/>
  </xs:attributeGroup>
  <!-- **** Element <actiondescription> **** -->
  <xs:complexType name="cm.actiondescription">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" ref="cmf.standardblock"/>
    </xs:sequence>
  </xs:complexType>
