  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.row.att">
    <xs:attributeGroup ref="attl.row"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.entry.att">
    <xs:attributeGroup ref="attl.entry"/>
  </xs:attributeGroup>
  <!-- **************** Include OASIS Exchange Table Model ****************** -->
  <!-- ************************ Element declarations ************************ -->
  <!-- **** Element <action> **** -->
