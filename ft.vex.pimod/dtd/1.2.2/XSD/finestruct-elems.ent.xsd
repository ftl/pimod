<!-- ************* Overwrite OASIS table parameter entities **************** -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:include schemaLocation="soextblx.xsd"/>
  <xs:complexType name="tbl.table.mdl">
    <xs:complexContent>
      <xs:extension base="cm.table"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:group name="tbl.entry.mdl">
    <xs:sequence>
      <xs:group ref="cm.entry"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="tbl.table.att">
    <xs:attributeGroup ref="attl.table"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.tgroup.att">
    <xs:attributeGroup ref="attl.tgroup"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.thead.att">
    <xs:attributeGroup ref="attl.thead"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.tbody.att">
    <xs:attributeGroup ref="attl.tbody"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tbl.colspec.att">
